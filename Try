--// Grow a Garden - Visual Dupe GUI Script by ScriptZy 

-- Create Screen GUI
local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local DupeButton = Instance.new("TextButton")
local CloseButton = Instance.new("TextButton")

ScreenGui.Parent = game.CoreGui
ScreenGui.Name = "GrowAGardenGUI"

Frame.Size = UDim2.new(0, 200, 0, 120)
Frame.Position = UDim2.new(0.5, -100, 0.5, -60)
Frame.BackgroundColor3 = Color3.fromRGB(34, 139, 34)
Frame.BorderSizePixel = 2
Frame.Parent = ScreenGui

DupeButton.Size = UDim2.new(0, 160, 0, 40)
DupeButton.Position = UDim2.new(0, 20, 0, 20)
DupeButton.Text = "üå± Dupe Items"
DupeButton.BackgroundColor3 = Color3.fromRGB(144, 238, 144)
DupeButton.Font = Enum.Font.GothamBold
DupeButton.TextSize = 16
DupeButton.Parent = Frame

CloseButton.Size = UDim2.new(0, 160, 0, 30)
CloseButton.Position = UDim2.new(0, 20, 0, 70)
CloseButton.Text = "‚ùå Close"
CloseButton.BackgroundColor3 = Color3.fromRGB(240, 128, 128)
CloseButton.Font = Enum.Font.Gotham
CloseButton.TextSize = 14
CloseButton.Parent = Frame

-- Dupe Function (Modify depending on the game method)
DupeButton.MouseButton1Click:Connect(function()
    local player = game.Players.LocalPlayer
    local garden = workspace:FindFirstChild("Garden") or workspace:FindFirstChild(player.Name .. "'s Garden")

    if garden then
        for _, item in pairs(garden:GetChildren()) do
            if item:IsA("Model") then
                local cloned = item:Clone()
                cloned.Parent = garden
                cloned:SetPrimaryPartCFrame(item:GetPrimaryPartCFrame() + Vector3.new(math.random(1,3), 0, math.random(1,3)))
            end
        end
        DupeButton.Text = "‚úÖ Duped!"
    else
        DupeButton.Text = "‚ùå Garden Not Found"
    end
end)

CloseButton.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)
